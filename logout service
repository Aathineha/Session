package com.example.sbi.service;

import com.example.sbi.model.LogoutRequest;
import org.springframework.stereotype.Service;

import java.util.List;

@Service
public class LogoutService {

    public String logoutDevices(List<LogoutRequest> requests) {
        for (LogoutRequest request : requests) {
            System.out.println("Logout: " + request.getNeId() + " | " + request.getIpAddress());
        }
        return "Logout successful";
    }
}